<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView android:text="ESTUDIANTE_MATERIA: "
        android:id="@+id/TextView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <EditText android:text=""
        android:id="@+id/txtEstudianteMateria"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TextView30"
        android:layout_alignParentStart="true">
    </EditText>


    <TextView android:text="ID PERSONA: "
        android:id="@+id/TextView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/txtEstudianteMateria"
        android:layout_alignParentStart="true">
    </TextView>

    <EditText android:text=""
        android:id="@+id/txtPersonas"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:password="true"
        android:layout_below="@+id/TextView31"
        android:layout_alignParentStart="true">
    </EditText>

    <TextView android:text="ID MAESTRO MATERIA: "
        android:id="@+id/TextView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/txtPersonas"
        android:layout_alignParentStart="true">
    </TextView>

    <EditText android:text=""
        android:id="@+id/txtMaestroMat"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:password="true"
        android:layout_below="@+id/TextView32"
        android:layout_alignParentStart="true">
    </EditText>

    <TextView android:text="ESTADO: "
        android:id="@+id/TextView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/txtMaestroMat"
        android:layout_alignParentStart="true">
    </TextView>

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinner" />

    <TextView android:text="MATRICULA: "
        android:id="@+id/TextView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/txtEstado"
        android:layout_alignParentStart="true">
    </TextView>

    <EditText android:text=""
        android:id="@+id/txtMatricula"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:password="true"
        android:layout_below="@+id/TextView34"
        android:layout_alignParentStart="true">
    </EditText>

    <TextView android:text="FECHA: "
        android:id="@+id/TextView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/txtMatricula"
        android:layout_alignParentStart="true">
    </TextView>

    <CalendarView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/calendarView2" />


    <TextView android:text="COSTO: "
        android:id="@+id/TextView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/txtFecha"
        android:layout_alignParentStart="true">
    </TextView>

    <EditText android:text=""
        android:id="@+id/txtCosto"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:password="true"
        android:layout_below="@+id/TextView36"
        android:layout_alignParentStart="true">
    </EditText>

</LinearLayout>